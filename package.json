  "scripts": {
    "build": "rm -rf dist && rm -rf public && yarn build:dev:client && yarn build:dev:server",
    "build:dev:server": "webpack --config webpack/webpack.server.js",
    "build:dev:client": "webpack --config webpack/webpack.client.js --mode=development",
    "build:prod": "rm -rf dist && rm -rf public && yarn build:prod:client && yarn build:prod:server",
    "build:prod:server": "NODE_ENV=production webpack --config webpack/webpack.server.js",
    "build:prod:client": "NODE_ENV=production webpack --config webpack/webpack.client.js --mode=production",
    "start:server": "nodemon ./dist/server.js",
    "dev:client": "webpack --config webpack/webpack.client.js --watch --mode development",
    "dev:build": "webpack --config webpack/webpack.server.js --watch",
    "start": "yarn build && run-p start:server dev:*",
    "tsc": "tsc",
    "test": "jest",
    "prettify": "prettier --write \"**/*.ts\" \"**/*.tsx\"",
    "test:watch": "jest --watch",
    "lint": "eslint --cache -c .eslintrc.js \"**/*.ts\" \"**/*.tsx\""
  },
